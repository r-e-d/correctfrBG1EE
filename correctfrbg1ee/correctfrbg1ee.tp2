BEGIN @4 DESIGNATED 40 // Suppression des ensembles de voix en anglais
	REQUIRE_PREDICATE GAME_IS ~bgee sod~ @0 // Necessite Baldur's Gate : Enhanced Edition (type de jeu incorrect)

	//BG1 Female 1-3 (anglais)
	ACTION_BASH_FOR ~lang/en_us/sounds~ ~^Female[4-6].*\.wav$~ BEGIN
		DELETE ~%BASH_FOR_FILESPEC%~
	END

	//BG1 Male 1-3 (anglais)
	ACTION_BASH_FOR ~lang/en_us/sounds~ ~^Male[4-6].*\.wav$~ BEGIN
		DELETE ~%BASH_FOR_FILESPEC%~
	END


	ACTION_IF FILE_EXISTS_IN_GAME ~BD0010.ARE~ THEN
	BEGIN
		//SoD Female 1-2 (anglais)
		ACTION_BASH_FOR ~lang/en_us/sounds~ ~^FEMALE[7-8].*\.wav$~ BEGIN
			DELETE ~%BASH_FOR_FILESPEC%~
		END

		//SoD Male 1-2 (anglais)
		ACTION_BASH_FOR ~lang/en_us/sounds~ ~^MALE[7-8].*\.wav$~ BEGIN
			DELETE ~%BASH_FOR_FILESPEC%~
		END
	END

	//AoN Andrew / Alveus / Baeloth / Dan / Elyse / Sarah / Trent (anglais)
	ACTION_BASH_FOR ~lang/en_us/sounds~ ~^bdtp.*\.wav$~ BEGIN
		DELETE ~%BASH_FOR_FILESPEC%~
	END

